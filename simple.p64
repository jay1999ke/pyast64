def print2darray(a, n, m):
    for i in range(n):
        for j in range(m):
            print("%d,", a[i * m + j])
        print("\n")
    print("\n")


def main():
    x = int[20][20]
    for i in range(20):
        for j in range(20):
            x[i][j] = i * 20 + j

    print2darray(x, 20, 20)
